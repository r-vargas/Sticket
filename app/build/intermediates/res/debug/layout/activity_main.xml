<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@drawable/main_menu"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/report"
        android:textSize="20dp"
        android:layout_width="match_parent"
        android:layout_alignParentBottom="true"
        android:layout_height="50dp"
        android:onClick="onClick"
        />

    <LinearLayout
        android:id="@+id/buttons"
        android:layout_above="@+id/report"
        android:paddingBottom="70dp"
        android:paddingLeft="25dp"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <Button
            android:layout_width="40dp"
            android:layout_height="35dp"
            android:tag="1"
            android:visibility="invisible"
            android:background="@drawable/sticket_number_one"
      />
        <Button

            android:layout_width="10dp"
            android:clickable="false"
            android:tag="-1"
            android:layout_height="wrap_content"
            android:visibility="invisible"/>
        <Button
            android:visibility="invisible"
            android:layout_width="40dp"
            android:layout_height="35dp"
            android:tag="2"
            android:background="@drawable/sticket_number_two"
            />

        <Button
            android:layout_width="10dp"
            android:clickable="false"
            android:tag="-1"
            android:layout_height="wrap_content"
            android:visibility="invisible"/>
        <Button
            android:visibility="invisible"
            android:layout_width="40dp"
            android:layout_height="35dp"
            android:tag="3"
            android:background="@drawable/sticket_number_three"
            />

        <Button
            android:layout_width="10dp"
            android:clickable="false"
            android:tag="-1"
            android:layout_height="wrap_content"
            android:visibility="invisible"/>

        <Button
            android:visibility="invisible"
            android:layout_width="40dp"
            android:layout_height="35dp"
            android:tag="4"
            android:background="@drawable/sticket_number_four"
            />
        <Button
            android:layout_width="10dp"
            android:clickable="false"
            android:tag="-1"
            android:layout_height="wrap_content"
            android:visibility="invisible"/>

        <Button
            android:visibility="invisible"
            android:layout_width="40dp"
            android:layout_height="35dp"
            android:tag="5"
            android:background="@drawable/sticket_number_five"
            />
        </LinearLayout>



</RelativeLayout>

